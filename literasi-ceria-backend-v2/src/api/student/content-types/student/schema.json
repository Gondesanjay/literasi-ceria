{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "nama_lengkap": {
      "type": "string",
      "required": true
    },
    "foto_profil": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "guru_pengajar": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "orang_tua_wali": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "plugin::users-permissions.user",
      "inversedBy": "students"
    },
    "school": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::school.school"
    },
    "total_bintang": {
      "type": "integer",
      "default": 0
    }
  }
}
